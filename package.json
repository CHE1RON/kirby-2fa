{
  "name": "kirby-2fa",
  "version": "1.1.0",
  "description": "Multifactor authentication for kirby panel",
  "main": "index.js",
  "scripts": {
    "dev": "npx -y kirbyup src/index.js --watch",
    "serve": "npx -y kirbyup serve src/index.js",
    "build": "npx -y kirbyup src/index.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src/components/*"
  },
  "keywords": [
    "kirby3",
    "kirby3-cms",
    "kirby3-plugin",
    "kirby3-panel",
    "2fa",
    "tfa"
  ],
  "author": "Graphic Market, Inc",
  "license": "MIT",
  "dependencies": {
    "components": "^0.1.0",
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.3"
  },
  "devDependencies": {
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.8.1"
  }
}
